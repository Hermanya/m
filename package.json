{
  "name": "m",
  "version": "1.0.0",
  "description": "Singleton models with Backbone and possibly other frameworks",
  "main": "m.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "backbone": "^1.1.2",
    "browserify": "^5.13.1",
    "jasmine-node": "^1.14.5",
    "jquery": "^2.1.3",
    "watchify": "^2.4.0",
    "uglify-js": "^2.4.16",
    "underscore": "^1.7.0"
  },
  "scripts": {
    "test": "jasmine-node test/ --matchall --color --autotest --watch `pwd`/src/",
    "build": "npm run build:min && npm run build:dev",
    "build:min": "node_modules/browserify/bin/cmd.js src/m-for-backbone.js -s M -x backbone | uglifyjs -c > m.min.js",
    "build:dev": "node_modules/browserify/bin/cmd.js src/m-for-backbone.js -s M -x backbone --debug --verbose > m.js",
    "watch": "watchify src/m-for-backbone.js -s M -x backbone --debug --verbose -o m.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hermanya/m.git"
  },
  "keywords": [
    "model",
    "backbone",
    "singleton"
  ],
  "author": "Herman Starikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hermanya/m/issues"
  },
  "homepage": "https://github.com/Hermanya/m"
}
